script.
  $(window).load(function(){
        fbcomment();
        $.getJSON('/galleriessee',function(dat){
            var mn='';
            $.each(dat,function(i,data){
              mn+='<li><a href="javascript:gallerysee(\''+data.image+'\')"><img src="/img/thumbs/'+data.image+'" alt="'+data.alt+'" width="75" height="75" /></a></li>';
            });
            $('.gallerylist').html(mn);
        })
    });
    function gallerysee(img){
      $('#myModal').modal('toggle');
      $('h4#myModalLabel.modal-title').html("Gallery Detail");
      $('.modal-body').html('<img class="col-md-12" src="/img/gallery/'+img+'" />');
      $('.modal-footer').html(" ");
    }
    function fbcomment(){
        var deswidth=$('.fblike').width()-10;
        $('.fb-like').attr('data-width',deswidth);
        $('.fb-like iframe').css('width',deswidth);
    }
    $(window).resize(function() {		
        fbcomment();
    })
    $('.dropdown').hover(function() {
        $(this).addClass('open');
    }, function() {
        $(this).removeClass('open');
    });
    $('.dropdown-sub').hover(function() {
        $(this).addClass('open');
    }, function() {
        $(this).removeClass('open');
    });

